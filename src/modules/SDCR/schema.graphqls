type TreemapLeaf {
  cid: String
  name: String!
  color: Float!
  size: Float
}

type SdcrData {
  scopeType: String
  scopeName: String
  groupType: String!
  data: [TreemapLeaf]
}

extend type Query {
  sdcr(
    scopeType: String
    scopeName: String
    groupType: String!
    dwelling: String
    type: String!
    startDate: String!
    endDate: String!
  ): SdcrData
}
